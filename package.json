    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.1",
    "tailwindcss": "^1.4.6"
  },
  "peerDependencies": {
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
